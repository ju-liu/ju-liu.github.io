clear all; clc; clf;

gamma = 0.9;
beta = 0.55;
xi = 0.5;

dt_T = 0.001 : 0.001 : 0.01;
dt_T = [ dt_T , 0.01 : 0.01 :

rho = zeros(length(dt_T));

for ii = 1 : length(dt_T)
    Omega = dt_T(ii) * 2 * pi;

    [eigen_1, eigen_2] = newmark_eigen(beta, gamma, Omega, xi);

    rho(ii) = abs(eigen_1);
    
    if( rho(ii) < abs(eigen_2) )
        rho(ii) = abs(eigen_2);
    end
    
end

semilogx(dt_T, rho);
